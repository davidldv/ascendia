{
  "expo": {
    "name": "ascendia",
    "slug": "ascendia-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ascendia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidlon.ascendia"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#05070C",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.davidlon.ascendia"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#05070C",
          "dark": {
            "backgroundColor": "#05070C"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e77699d5-e092-46f0-b164-00b27daa6d47"
      }
    },
    "owner": "davidl.on",
    "updates": {
      "url": "https://u.expo.dev/e77699d5-e092-46f0-b164-00b27daa6d47"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
